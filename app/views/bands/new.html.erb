<h1>Create a new band!</h1>
<% flash[:errors] && flash[:errors].each do |error| %>
<%= error %>
<% end %>
<form class="" action="<%= bands_url %>" method="post">
<input type="hidden" name="authenticity_token" value=" <%= form_authenticity_token %>">
<label for="band_name">Name:</label>
<input type="text" name="band[name]" value=" <%= params[:name] %>" id="band_name">
<input type="submit"  value="Create band!">
</form>
